<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_people"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_people"
        android:name="com.aslanovaslan.firemessage.ui.people.PeopleFragment"
        android:label="@string/title_people"
        tools:layout="@layout/fragment_people" />

    <fragment
        android:id="@+id/navigation_my_account"
        android:name="com.aslanovaslan.firemessage.ui.myAccount.MyAccountFragment"
        android:label="@string/title_my_account"
        tools:layout="@layout/fragment_my_account" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.aslanovaslan.firemessage.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
</navigation>